<div class="row">
  <div class="col-md-6">
    <h2 class="my-4">店舗登録</h2>
    <%= form_with model: @store do |f| %>
      <%= render 'layouts/error_messages', model: f.object %>
      <%= render 'form', f: f %>
      <%= f.submit '登録', class: "btn btn-primary btn-block mt-4" %>
    <% end %>
  </div>
</div>